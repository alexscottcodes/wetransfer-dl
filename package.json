{
  "name": "wetransfer-downloader",
  "version": "1.0.0",
  "description": "Universal JavaScript library for downloading WeTransfer files (supports Node.js and browsers)",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wetransfer",
    "download",
    "file-transfer",
    "universal",
    "isomorphic"
  ],
  "author": "Alexander P. Scott",
  "license": "GPL-3.0-only",
  "dependencies": {
    "axios": "^1.8.0",
    "follow-redirects": "^1.15.11",
    "function-bind": "^1.1.2",
    "get-intrinsic": "^1.3.0",
    "gopd": "^1.2.0",
    "picomatch": "^4.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.10.2",
    "rollup": "^4.30.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  }
}
